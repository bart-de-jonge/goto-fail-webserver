<!DOCTYPE html>
<html lang="en">
    <head>
        <% include ./partials/head %>
        <link rel="import" href="../bower_components/polymer/polymer.html">
        <link rel="import" href="../bower_components/file-upload/file-upload.html">
        <link rel="import" href="../bower_components/paper">
        <link rel="import" href="../bower_components/iron-flex-layout/iron-flex-layout-classes.html">
        <link rel="import" href="../bower_components/paper-header-panel/paper-header-panel.html">
        <link rel="import" href="../bower_components/paper-drawer-panel/paper-drawer-panel.html">
        <link rel="import" href="../bower_components/paper-toolbar/paper-toolbar.html">
        <link rel="import" href="../bower_components/paper-icon-button/paper-icon-button.html">
        <link rel="import" href="../bower_components/paper-button/paper-button.html">
        <link rel="import" href="../bower_components/paper-toast/paper-toast.html">
        <link rel="import" href="../bower_components/iron-icons/iron-icons.html/head">
        <link rel="import" href="../elements/gotofail-drawer.html">
    </head>
<body>
    <paper-drawer-panel peeking force-narrow left-drawer id="drawer-panel">

        <gotofail-drawer drawer id="drawer"></gotofail-drawer>

        <paper-header-panel main>
            <div class="paper-header">
                <paper-toolbar justify="left" id="head" style="height: 60px;">
                    <paper-icon-button icon="menu" paper-drawer-toggle></paper-icon-button>
                    <h2 style="font-weight: lighter">Project File Upload</h2>
                </paper-toolbar>
            </div>

            <p>
                <file-upload droppable="true" raised="true" method="post">Please Select A Project File</file-upload>
            </p>
        </paper-header-panel>
    </paper-drawer-panel>
</body>
</html>
